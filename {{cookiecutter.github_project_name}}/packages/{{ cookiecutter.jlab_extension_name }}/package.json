{
  "name": "{{ cookiecutter.jlab_extension_name }}",
  "version": "0.1.0",
  "description": "A JupyterLab extension for {{ cookiecutter.python_package_name }}. {{ cookiecutter.project_short_description }}",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "dependencies": {
    "@jupyterlab/application": "^0.10.0",
    "@phosphor/coreutils": "^1.3.0",
    "@jupyter-widgets/jupyterlab-manager": "^0.27.0",
    "{{ cookiecutter.npm_package_name }}": "^{{ cookiecutter.npm_package_version }}"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.2"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab extension",
    "jupyterlab notebook"
  ],
  "jupyterlab": {
    "extension": true
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc --project src",
    "prepublish": "npm run clean && npm run build"
  },
  "files": [
    "build/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/{{ cookiecutter.github_organization_name }}/{{ cookiecutter.python_package_name }}"
  },
  "author": "{{ cookiecutter.project_short_description }}",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/{{ cookiecutter.github_organization_name }}/{{ cookiecutter.python_package_name }}/issues"
  },
  "homepage": "https://github.com/{{ cookiecutter.github_organization_name }}/{{ cookiecutter.python_package_name }}"
}
